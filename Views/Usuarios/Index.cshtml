@model Usuario
<div class="border p-3 mt-4">
    <div class="row pb-2">
        <h2 class="text-primary">TU INFORMACION</h2>
        <hr />
    </div>
    @*< div asp-validation-summary="All"></div> *@
    <div class="mb-3">
        <label asp-for="Id"></label>
        <input asp-for="Id" disabled class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Nombre_Completo"></label>
        <input asp-for="Nombre_Completo" disabled class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Email"></label>
        <input asp-for="Email" disabled class="form-control" />
    </div>
    <div class="mb-3">
        <label asp-for="Rol"></label>
        <input asp-for="Rol" disabled class="form-control" />
    </div>
    <div class="row">
        <div class="col">
            <a type="button" class="btn btn-primary" asp-controller="Usuarios" asp-action="Edit" asp-route-Id="@Model.Id">Editar Perfil</a>
        </div>
        <div class="col">
            <a type="button" class="btn btn-danger" asp-controller="Usuarios" asp-action="Delete" asp-route-Id="@Model.Id">Borrar Perfil</a>
        </div>
    </div>
</div>